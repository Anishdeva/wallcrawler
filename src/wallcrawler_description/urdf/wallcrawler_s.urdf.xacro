<?xml version="1.0"?>
<robot xmlns:xacro="http://www.ros.org/wiki/xacro" name="wallcrawler3">
    <!-- Define material (colors) -->
    <material name="blue">
        <color rgba="0 0 0.8 1"/>
    </material>

    <material name="white">
        <color rgba="1 1 1 1"/>
    </material>

    <material name="black">
        <color rgba="0.2 0.2 0.2 1"/>
    </material>

    <material name="red">
        <color rgba="0.8 0 0 1"/>
    </material>

    <!-- Define base link (Choosing center segment) -->
    <link name="base_link">
        <visual>
        <geometry>
		<mesh filename="package://wallcrawler_description/meshes/dae_new/Payload_box.dae"/>
        </geometry>
	<origin rpy="1.57 0 0" xyz="0 0.0 0.0"/>
        <material name="red"/>
        </visual>
        <collision>
        <geometry>
        	<mesh filename="package://wallcrawler_description/meshes/dae_new/Payload_box.dae"/>
        </geometry>
	<origin rpy="1.57 0 0" xyz="0 0.0 0.0"/>
        </collision>
        <inertial>
        <mass value="30.29"/>
        <inertia ixx="101083.43" ixy="-39.13" ixz="35.56" iyy="444100.5" iyz="-10.33" izz="429769"/>        
        </inertial>                
    </link>

  <!-- Wheels attached to base/central link -->
    <link name="center_right_wheel">
        <visual>
        <geometry>
            <mesh filename="package://wallcrawler_description/meshes/dae_new/Wheel.dae"/>
        </geometry>
        <origin rpy="0 1.57 0" xyz="0 0.0 0.0"/>
        <material name="blue"/>
        </visual>
        <collision>
      <geometry>
            <mesh filename="package://wallcrawler_description/meshes/dae_new/Wheel.dae"/>
        </geometry>
        <origin rpy="0 1.57 0" xyz="0 0.0 0.0"/>
        </collision>
        <inertial>
        <origin rpy="0 1.57 0" xyz="0 0.0 0.0"/>
        <mass value="0.57"/>
        <inertia ixx="97.07" ixy="0" ixz="0" iyy="185.68" iyz="0.00" izz="97.07"/>
        </inertial>                
    </link>

    <joint name="center_right_wheel_joint" type="continuous">
        <axis xyz="0 1 0" rpy="0 1.57 0"/>
        <!-- <limit effort="1000.0" lower="0.0" upper="0.548" velocity="0.5"/> -->
        <parent link="base_link"/>

        <child link="center_right_wheel"/>
        <origin xyz="-20 -83 83" rpy="1.57 1.57 0"/>
    </joint>

    <gazebo reference="center_right_wheel">
        <mu1 value="200.0"/>
        <mu2 value="100.0"/>
        <kp value="10000000.0"/>
        <kd value="1.0"/>
    </gazebo>

    <transmission name="center_right_wheel_transmission">
        <type>transmission_interface/SimpleTransmission</type>
        <joint name="center_right_wheel_joint">
            <hardwareInterface>VelocityJointInterface</hardwareInterface>
        </joint>
        <actuator name="center_right_wheel_actuator">
            <mechanicalReduction>7</mechanicalReduction>
            <hardwareInterface>VelocityJointInterface</hardwareInterface>
        </actuator>
    </transmission>

